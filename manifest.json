{
	"manifest_version": 2,

	"name": "Scraper Utiity",
	"description": "This extension extracts websites' data and pipes it to a database",
	"version": "0.0.1",

	"browser_action":{
		"default_icon": "icon.png"
	},
	"permissions": [
        "webRequest",
		"webRequestBlocking",
		"activeTab",
		"http://192.168.1.62:3000/queries"
	],
	"background": {
	    "scripts": ["js/jquery-3.2.1.min.js","js/background-script.js"]
	},
	"web_accessible_resources": [
		"html/*",
		"css/*",
		"js/*"
	]
}
